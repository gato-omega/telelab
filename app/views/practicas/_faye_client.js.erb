// Faye channels
var channels = '<%= channels.join ',' %>'.split(',');

$(function()
{
    var faye_client = new Faye.Client('<%= faye_server_url %>');
    channels.forEach(function(channel){
        faye_client.subscribe(channel, function(data){
            eval(data);
        });
    });

